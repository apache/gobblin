job.name=testJob

source.class=org.apache.gobblin.multistage.source.HttpSource
converter.classes=org.apache.gobblin.converter.avro.JsonIntermediateToAvroConverter

ms.extractor.class=org.apache.gobblin.multistage.extractor.JsonExtractor
ms.http.client.factory=org.apache.gobblin.multistage.factory.ApacheHttpClientFactory

extract.namespace=test
extract.table.name=REPLACEME
extract.table.type=SNAPSHOT_ONLY
extract.is.full=true
dataset.urn=test.table

ms.authentication={"method": "basic", "encryption": "base64", "header": "Authorization"}
ms.data.field=results
ms.http.request.method=GET
ms.http.request.headers={"Content-Type": "application/json"}
ms.watermark=[{"name": "system","type": "datetime", "range": {"from": "2020-01-01", "to": "2020-01-31"}}]
ms.output.schema=
ms.parameters=[{"name": "$format", "type": "list", "value": "json"}]
ms.source.uri=
ms.session.key.field=
ms.total.count.field=

fs.uri=file://localhost/
state.store.fs.uri=file://localhost/
data.publisher.final.dir=/tmp/gobblin/job-output

writer.destination.type=HDFS
writer.output.format=AVRO

source.conn.username=
source.conn.password=
