      "test": "ligradle test -PallTests=true -Dtest.excludeGroups=slow,investigate"
    },
    "gradle_opts": "-Xmx2048m -Xms1024m -XX:PermSize=128m -XX:MaxPermSize=512m",
    "type": "gradle",
    "versions": {
      "gradle": "1.8",
      "rum": "2.*.*"
    }
  },
