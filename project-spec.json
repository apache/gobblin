    "commonsLang": "commons-lang:commons-lang:2.4",
    "commonsLogging": "commons-logging:commons-logging:1.1",
    "commonsConfiguration": "commons-configuration:commons-configuration:1.10",
    "guava": "com.google.guava:guava:15.0",
